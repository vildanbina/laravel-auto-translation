{
    "name": "vildanbina/laravel-auto-translation",
    "description": "A Laravel package for automating the translation of language files.",
    "keywords": [
        "laravel",
        "translations",
        "localization",
        "language files",
        "automation"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "Vildan Bina",
            "email": "vildanbina@gmail.com",
            "homepage": "https://www.vildanbina.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": ">=8.0",
        "mehrab-wj/tiktoken-php": "^1.0",
        "laravel/framework": "^9.0|^10.0|^11.0|^12.0",
        "illuminate/support": "^9.0|^10.0|^11.0|^12.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5",
        "orchestra/testbench": "^7.0|^8.0|^9.0|^10.0",
        "guzzlehttp/guzzle": "^7.10"
    },
    "autoload": {
        "psr-4": {
            "VildanBina\\LaravelAutoTranslation\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "VildanBina\\LaravelAutoTranslation\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "VildanBina\\LaravelAutoTranslation\\AutoTranslationsServiceProvider"
            ],
            "aliases": {
                "LaravelAutoTranslation": "VildanBina\\LaravelAutoTranslation\\Facades\\LaravelAutoTranslation"
            }
        }
    }
}
